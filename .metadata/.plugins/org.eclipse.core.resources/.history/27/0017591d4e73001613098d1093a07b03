public class Arrays {

	public static void main(String[] args) {
		
	}
	
	public static int sum(int[] arr){
		return IntStream.of(arr).sum();
	}
	
	public static String toString(int[] arr){
		String str = "";
		for(int i : arr){
			 str = i + str;
		}
	     
		return "n = " + str;
	}
	
