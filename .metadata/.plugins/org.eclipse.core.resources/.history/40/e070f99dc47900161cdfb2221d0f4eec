 */
public class Bicycle extends Vehicle {
	private final static int SPACE_REQUIRED = 1;
	private final static double PASSENGER_COST = 20.0;
	private final static double VEHICLE_COST = 20.0;
	private final static int MAX_NUMBER_OF_PASSENGERS = 4;

	public Bicycle(int numberOfPassengers) {
		super(PASSENGER_COST, VEHICLE_COST,SPACE_REQUIRED, MAX_NUMBER_OF_PASSENGERS, numberOfPassengers);
	}

}
