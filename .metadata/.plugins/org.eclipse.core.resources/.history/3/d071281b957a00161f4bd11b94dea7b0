 */
public class Lorry extends Vehicle {
	private final static int SPACE_REQUIRED = 40;
	private final static double PASSENGER_COST = 15.0;
	private final static double VEHICLE_COST = 300.0;
	private final static int MAX_NUMBER_OF_PASSENGERS = 2;

	public Lorry(int numberOfPassengers) {
		super(PASSENGER_COST, VEHICLE_COST,SPACE_REQUIRED, MAX_NUMBER_OF_PASSENGERS, numberOfPassengers);
	}

}
